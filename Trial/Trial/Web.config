<configuration>
  <connectionStrings>
    <add name="OFFICIAL-DB" connectionString="Data Source=.\SQLEXPRESS;AttachDbFilename=&quot;C:\Users\Shiela\Desktop\MP-192\Trial\Trial\App_Data\Direct-Selling.mdf&quot;;Integrated Security=True;Connect Timeout=30;User Instance=True"
      providerName="System.Data.SqlClient" />
